{
  "dashboard": {
    "title": "Vritti Mission Metrics - Repeat Error Prevention",
    "description": "Mission-critical dashboard tracking Vritti's core value proposition: preventing AI agents from repeating mistakes",
    "tags": [
      "vritti",
      "mission",
      "prevention",
      "gating"
    ],
    "timezone": "utc",
    "refresh": "10s",
    "schemaVersion": 36,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "ðŸŽ¯ Mission Success: Repeat Errors Prevented (24h)",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 8,
          "h": 6
        },
        "targets": [
          {
            "expr": "increase(episodic_memory_repeat_error_prevented_total[24h])",
            "legendFormat": "Errors Prevented",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 10,
                  "color": "yellow"
                },
                {
                  "value": 50,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "justifyMode": "center"
        },
        "description": "Total number of repeat errors prevented by Vritti in the last 24 hours. This is THE mission metric."
      },
      {
        "id": 2,
        "type": "stat",
        "title": "ðŸ“Š Mission Success Rate (%)",
        "gridPos": {
          "x": 8,
          "y": 0,
          "w": 8,
          "h": 6
        },
        "targets": [
          {
            "expr": "(increase(episodic_memory_repeat_error_prevented_total[24h]) / increase(episodic_memory_gating_decision_total[24h])) * 100",
            "legendFormat": "Prevention Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 60,
                  "color": "orange"
                },
                {
                  "value": 80,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "description": "Percentage of gating decisions that prevented repeat errors. Target: >80%"
      },
      {
        "id": 3,
        "type": "gauge",
        "title": "âš¡ Real-Time Prevention Rate",
        "gridPos": {
          "x": 16,
          "y": 0,
          "w": 8,
          "h": 6
        },
        "targets": [
          {
            "expr": "episodic_memory_repeat_error_prevention_rate",
            "legendFormat": "{{customer_tier}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 0.7,
                  "color": "yellow"
                },
                {
                  "value": 0.8,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "description": "Real-time mission success rate gauge. Alert threshold: <70%"
      },
      {
        "id": 4,
        "type": "timeseries",
        "title": "ðŸ“ˆ Repeat Errors Prevented Over Time",
        "gridPos": {
          "x": 0,
          "y": 6,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(episodic_memory_repeat_error_prevented_total[5m])",
            "legendFormat": "Prevention Rate (per second)",
            "refId": "A"
          },
          {
            "expr": "rate(episodic_memory_gating_decision_total[5m])",
            "legendFormat": "Total Gating Decisions",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "ops"
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "calcs": [
              "mean",
              "last"
            ]
          }
        },
        "description": "Time series showing prevention rate vs total gating decisions"
      },
      {
        "id": 5,
        "type": "bargauge",
        "title": "ðŸ”¥ Top Error Classes Prevented",
        "gridPos": {
          "x": 12,
          "y": 6,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "topk(10, sum by (error_class) (increase(episodic_memory_repeat_error_prevented_total[24h])))",
            "legendFormat": "{{error_class}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "blue"
                },
                {
                  "value": 10,
                  "color": "yellow"
                },
                {
                  "value": 50,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "description": "Top 10 error classes where Vritti prevented repeats"
      },
      {
        "id": 6,
        "type": "table",
        "title": "ðŸ“‹ Prevention Breakdown by Recommendation Type",
        "gridPos": {
          "x": 0,
          "y": 14,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (recommendation) (increase(episodic_memory_repeat_error_prevented_total[24h]))",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "recommendation": "Recommendation",
                "Value": "Count"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Count",
              "desc": true
            }
          ]
        },
        "description": "Breakdown of preventions by gating recommendation type (BLOCK/REWRITE/HINT)"
      },
      {
        "id": 7,
        "type": "piechart",
        "title": "ðŸ¥§ Customer Tier Distribution",
        "gridPos": {
          "x": 12,
          "y": 14,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (customer_tier) (increase(episodic_memory_repeat_error_prevented_total[24h]))",
            "legendFormat": "{{customer_tier}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": [
              "sum"
            ]
          },
          "pieType": "pie",
          "displayLabels": [
            "percent"
          ]
        },
        "description": "Distribution of prevented errors across customer tiers"
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "âš¡ Gating Latency P99 (Target: <100ms)",
        "gridPos": {
          "x": 0,
          "y": 22,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(episodic_memory_gating_latency_seconds_bucket[5m])) by (le)) * 1000",
            "legendFormat": "P99 Latency",
            "refId": "A"
          },
          {
            "expr": "100",
            "legendFormat": "SLO Target (100ms)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 100,
                  "color": "yellow"
                },
                {
                  "value": 200,
                  "color": "red"
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "SLO Target (100ms)"
              },
              "properties": [
                {
                  "id": "custom.lineStyle",
                  "value": {
                    "dash": [
                      10,
                      10
                    ],
                    "fill": "dash"
                  }
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            }
          ]
        },
        "description": "P99 latency for gating decisions. SLO: <100ms"
      },
      {
        "id": 9,
        "type": "stat",
        "title": "ðŸ’° Estimated Value Delivered ($/day)",
        "gridPos": {
          "x": 12,
          "y": 22,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "increase(episodic_memory_repeat_error_prevented_total[24h]) * 10",
            "legendFormat": "Value ($10/error prevented)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "blue"
                },
                {
                  "value": 100,
                  "color": "green"
                },
                {
                  "value": 1000,
                  "color": "dark-green"
                }
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background"
        },
        "description": "Estimated value delivered by preventing developer time waste. Assumes $10/error (30 min debugging @ $20/hr)"
      },
      {
        "id": 10,
        "type": "timeseries",
        "title": "ðŸš¨ Mission Health Score (Composite)",
        "gridPos": {
          "x": 0,
          "y": 30,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "(\n  (increase(episodic_memory_repeat_error_prevented_total[5m]) > 0) * 0.4 +\n  (histogram_quantile(0.99, sum(rate(episodic_memory_gating_latency_seconds_bucket[5m])) by (le)) < 0.1) * 0.3 +\n  (rate(episodic_memory_errors_total{error_type=\"gating\"}[5m]) < 0.01) * 0.3\n) * 100",
            "legendFormat": "Health Score (%)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 70,
                  "color": "yellow"
                },
                {
                  "value": 90,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "description": "âš ï¸ NOTE: This uses BINARY composite scoring (each term = 0 or 1, then weighted 40/30/30), NOT continuous metrics. Possible scores: 0%, 30%, 60%, or 100%. A score of 40 means ONLY prevention passed (not 40% continuous health). Score of 60 = prevention + one other component passed. Score of 100 = all components passing. For continuous health visibility, consider refactoring to use actual rate-based metrics."
      }
    ],
    "templating": {
      "list": [
        {
          "name": "customer_tier",
          "label": "Customer Tier",
          "type": "query",
          "query": "label_values(episodic_memory_repeat_error_prevented_total, customer_tier)",
          "multi": true,
          "includeAll": true,
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "time_range",
          "label": "Time Range",
          "type": "interval",
          "query": "5m,15m,1h,6h,24h,7d",
          "current": {
            "selected": true,
            "text": "1h",
            "value": "1h"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(up{job=\"vritti\"}[1m]) > 0",
          "tagKeys": "version",
          "textFormat": "Deployment",
          "titleFormat": "Version {{version}}"
        }
      ]
    }
  }
}